{
  "name": "emtech-release-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://release-notes.emtech.com",
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "sass": "^1.69.5",
    "tailwind-merge": "^2.1.0",
    "typescript": "^4.4.2",
    "validator": "^13.11.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "build": "env-cmd -f .env.development react-scripts build",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "build:dev-ssr": "env-cmd -f .env.development webpack --config webpack.server.js --mode development",
    "start:dev-ssr": "env-cmd -f .env.development node ./server-build/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/validator": "^13.11.6",
    "css-loader": "^7.1.1",
    "gh-pages": "^6.1.0",
    "prettier": "^3.0.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.3.5",
    "ts-loader": "^9.5.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
